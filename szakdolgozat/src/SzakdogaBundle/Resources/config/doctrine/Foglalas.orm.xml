<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SzakdogaBundle\Entity\Foglalas" table="foglalas">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="userId" type="integer" column="user_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="szobaId" type="integer" column="szoba_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="mettol" type="datetime" column="mettol" nullable="false"/>
    <field name="meddig" type="datetime" column="meddig" nullable="false"/>
    <field name="igeny" type="text" column="igeny" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
